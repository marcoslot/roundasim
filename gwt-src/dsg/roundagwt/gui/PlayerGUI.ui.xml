<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="res" type="dsg.roundagwt.gui.SimGUIResources"/>

    <g:HTMLPanel>
        <g:Button ui:field="stopButton">Stop</g:Button>
        <g:Button ui:field="startButton">Start</g:Button>
        <g:Button addStyleNames="{res.style.hidden}" ui:field="pauseButton">Pause</g:Button>
    </g:HTMLPanel>
</ui:UiBinder> 